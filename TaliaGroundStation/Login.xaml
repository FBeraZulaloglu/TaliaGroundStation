<Window x:Class="TaliaGroundStation.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaliaGroundStation"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <Image Source="TaliaLogo.png" Margin="22,23,630,305" Opacity="100"></Image>
        <Label Content="Password" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="219,170,0,0" Width="69"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Margin="308,178,0,0" VerticalAlignment="Top" Width="125"/>
        <Button Content="Set Password" HorizontalAlignment="Left" Margin="308,220,0,0" VerticalAlignment="Top" Width="98" Height="25" Click="CreatePassword"/>
        <Button Content="Main Window" HorizontalAlignment="Left" Margin="457,170,0,0" VerticalAlignment="Top" Width="95" Height="27" Click="OpenMainWindow"/>
    </Grid>
</Window>
